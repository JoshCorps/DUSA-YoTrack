            div(class="calendar")
                - if (data)
                    - for (var week = 0; week < 5; week++)
                        div(class="calendar-week row-dynamic")
                            - for(var cell = (7 * week); cell < (7 * week) + 7; cell++)
                                - if (data[cell])
                                    - if (data[cell].faded)
                                        a(class="calendar-day no-day" href="#")
                                        div
                                    - else
                                        a(class="calendar-day" href="#" style="color: black; text-decoration:none; background: " + data[cell].color)
                                            div= data[cell].dayNumber
